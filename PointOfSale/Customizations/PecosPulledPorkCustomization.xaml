<UserControl x:Class="PointOfSale.Customizations.PecosPulledPorkCustomization"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PointOfSale.Customizations"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="10">
        <CheckBox IsChecked="{Binding Path=Bread}">Bread</CheckBox>
        <CheckBox IsChecked="{Binding Path=Pickle}">Pickle</CheckBox>
    </StackPanel>
</UserControl>
